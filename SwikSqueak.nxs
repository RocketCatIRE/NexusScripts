{"name":"swiksqueak","enabled":true,"description":"Only MAP please!","type":"group","id":1,"items":[{"type":"function","name":"onLoad","enabled":true,"id":2,"code":"ssq = typeof ssq != 'undefined' ? ssq : {}\n\nssq.use   = false\nssq.state = ssq.state || 0\n\nssq.mapOnly = function() {\n  if (!ssq.use) { return }\n  var t = ['map', 'all_comm', 'inventory', 'room', 'affdef', 'skills', 'who', 'quests']\n  for (var i = 0; i < t.length; i++) {\n    client.move_tab_to_existing_container(t[i], 'container_1', i)   \n  }\n  ssq.state = 1\n}\n\nssq.normalise = function() {\n  if (!ssq.use) { return }\n  var m = {\n    ['container_1'] : ['map'],\n    ['container_2'] : ['all_comm', 'affdef', 'skills', 'who', 'quests'],\n    ['container_3'] : ['room', 'inventory'],\n  }\n  for (var k in m) {\n    var t = m[k]\n    for (var i = 0; i < t.length; i++) {\n      client.move_tab_to_existing_container(t[i], k, i)   \n    }\n  }\n  ssq.state = 0\n}"},{"type":"function","name":"onGMCP","enabled":true,"id":3,"code":"if (typeof ssq != 'undefined' && args.gmcp_method == 'Room.Info') {\n  if (args.gmcp_args.num == -2 && args.gmcp_args.environment == 'bridge') {\n    if (ssq.state == 1) { // do nothing\n    } else {\n      ssq.mapOnly()\n    }\n  } else if (ssq.state == 1) {\n    ssq.normalise()\n  }\n}"},{"type":"alias","name":"flymap","enabled":true,"id":4,"matching":"regexp","whole_words":true,"case_sensitive":true,"prefix_suffix":true,"actions":[{"action":"script","script":"if (typeof ssq != 'undefined') {\n  ssq.use = false\n  ssq.mapOnly()\n}"}],"text":"^flymap$"},{"type":"alias","name":"landmap","enabled":true,"id":5,"matching":"regexp","whole_words":true,"case_sensitive":true,"prefix_suffix":true,"actions":[{"action":"script","script":"if (typeof ssq != 'undefined') {\n  ssq.use = false\n  ssq.normalise()\n}"}],"text":"^landmap$"},{"type":"alias","name":"ssq auto","enabled":true,"id":6,"matching":"regexp","whole_words":true,"case_sensitive":true,"prefix_suffix":true,"actions":[{"acti